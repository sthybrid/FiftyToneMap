<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sthybrid.fiftytonemap"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activity.MainMenu">
            <intent-filter >
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.FiftyToneMap"></activity>
        <activity android:name=".activity.SelfTest"></activity>
        <activity android:name=".activity.ErrorStatistics"></activity>
    </application>

</manifest>
